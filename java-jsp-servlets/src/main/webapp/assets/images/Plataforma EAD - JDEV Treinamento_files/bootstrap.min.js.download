	<script type="text/javascript">

function removerEspacoBranco() {
	
	document.getElementById("username").value = document.getElementById("username").value.trim();
	
	document.getElementById("password").value = document.getElementById("password").value.trim();
	
}

  function recuperaracesso() {
		var username = document.getElementById("username").value;
		
		if (username == '' || username == null || username.trim() == ''){
			alert('Informe seu e-mail de login.')
			return false;
		}
	  
	  if(confirm('Sua senha será redefinida!\nConfirme no "OK"')){
	  
		
		var token = $("meta[name='_csrf']").attr("content");
		 var header = $("meta[name='_csrf_header']").attr("content");
		 
			
	   if (username != null && username.trim() !== '') {
		 $.ajax({
			 beforeSend: function (jqXHR, settings) {
			        jqXHR.setRequestHeader(header, token);
			 },
			  method: "POST",
			  url: "recuperarAcesso", // para qual servlet?
			  data: {username: username}
			})
			.success(function(response) {// resposta ok - nenhum erro
			    alert(response);
		   })
		   .fail(function(xhr, status, errorThrown) { // resposta erro - algum problema ocorreu
			   alert("Error: " + xhr.responseText); // mostra resposta do servidor
		   });
	 
	}
  }
 }
  
  
  function validEmail(value) {  
	    var valid = true;  
	    var emails = value.replace(';', ',').split(",");  
	  
	    $.each(emails, function () {  
	        if ($.trim(this) != '')  
	        {  
	            if (!$.trim(this).match(/^([\w\.\-]+)@([\w\-]+)((\.(\w){2,3})+)$/i))  
	                valid = false;  
	        }  
	    });  
	    return valid;  
	} 
  
  
 function criarContaTesteAluno() {
	 
	 var email = document.getElementById("email").value;
	 var nome = document.getElementById("nome").value;
	 var phone = document.getElementById("phone").value;
	 
	 if (nome == '' || nome == null || nome.trim() == ''){
			alert('Informe seu nome.')
			document.getElementById("nome").focus();
			return false;
		}
		
		if (email == '' || email == null || email.trim() == ''){
			alert('Informe seu e-mail.')
			document.getElementById("email").focus();
			return false;
		}
		
		
		if (phone == '' || phone == null || phone.trim() == '' || phone.length < 10){
			alert('Informe seu telefone.')
			document.getElementById("phone").focus();
			return false;
		}
		
		if (!validEmail(email)){
			alert('Informe seu e-mail.')
			document.getElementById("email").focus();
			return false;
		}
		
		
		var token = $("meta[name='_csrf']").attr("content");
		var header = $("meta[name='_csrf_header']").attr("content");
				
				 $.ajax({
					 beforeSend: function (jqXHR, settings) {
					        jqXHR.setRequestHeader(header, token);
					 },
					  method: "POST",
					  url: "criarContaTesteAluno", // para qual servlet?
					  data: {email: email, nome: nome, phone : phone}
					})
					.success(function(response) {// resposta ok - nenhum erro
						
						if (response.existe == false || response.existe == 'false') {
							
							 document.getElementById("username").value = response.login;
							 document.getElementById("password").value = response.senha;
							 
							 removerEspacoBranco();
							 
							 fbq('track', 'Lead');
				         		
				         	 evento_lead_google_ads();
							 
							 $("#formlogineaa" ).submit();
							 
						}else {
							
							document.getElementById('msgCadGratuito').textContent = response.msg;
							
						}
						 
				   })
				   .fail(function(xhr, status, errorThrown) { // resposta erro - algum problema ocorreu
					   alert("Error: " + xhr.responseText); // mostra resposta do servidor
				   });
				
		  

} 
</script>	




